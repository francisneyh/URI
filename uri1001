
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) {
         int A, B, X;
        Scanner entrada = new Scanner(System.in);
        
        A = entrada.nextInt();
        B = entrada.nextInt();
        
        X = A + B;
        
        System.out.println("X = " + X);
 
    }
 
}		

